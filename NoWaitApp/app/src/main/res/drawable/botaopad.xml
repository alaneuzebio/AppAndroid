<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#d1c9c9" />
            <gradient android:angle="-90" android:startColor="#3e3e3e" android:endColor="#595959"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#303030" />
            <solid android:color="#1b1a1a"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#0a0909" />
            <gradient android:angle="-90" android:startColor="#9f9f9f" android:endColor="#9c9c9c" />
        </shape>
    </item>
</selector>